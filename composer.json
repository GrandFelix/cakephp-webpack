{
    "name": "grandfelix/cakephp-webpack",
    "description": "Webpack plugin for CakePHP",
    "authors": [{
      "name": "GrandFelix",
      "email": "grandelix@gmail.com",
      "role": "Author"
    }],
    "support": {
      "issues": "https://github.com/GrandFelix/cakephp-webpack/issues",
      "source": "https://github.com/GrandFelix/cakephp-webpack"
    },
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": ">=3.2.9 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Webpack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webpack\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
